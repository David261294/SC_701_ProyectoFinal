@model IEnumerable<FE.W.Models.FacturaPaciente>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaFactura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descuento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Iva)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPacienteNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFactura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descuento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Iva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPacienteNavigation.ApellidoPaciente)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodigoFactura">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodigoFactura">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodigoFactura">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
