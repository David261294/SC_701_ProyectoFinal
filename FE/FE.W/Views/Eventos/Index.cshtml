@model IEnumerable<FE.W.Models.Eventos>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DíaEvento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MesEvento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AñoEvento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DetallesEvento)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DíaEvento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MesEvento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AñoEvento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DetallesEvento)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdEvento">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdEvento">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdEvento">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
