@model IEnumerable<FE.W.Models.Contacto>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreContacto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApellidoContacto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CorreoContacto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DireccionContacto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TelefonoContacto)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreContacto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApellidoContacto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CorreoContacto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DireccionContacto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TelefonoContacto)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdContacto">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdContacto">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdContacto">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
