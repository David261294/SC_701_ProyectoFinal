@model IEnumerable<FE.Models.Dentista>

@{
    ViewData["Title"] = "Index";
}

<h1>Lista de Datos</h1>

<p>
    <a class="btn btn-primary" href="~/Dentistas/Create">Crear Nuevo</a>
    @*<a asp-action="Create">Create New</a>*@
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreDentista)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApellidoDentista)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contraseña)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NombreDentista)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ApellidoDentista)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Contraseña)
                </td>
                <td>
                    <a class="btn btn-warning" href="~/Dentistas/Edit/@item.IdDentista "> Editar </a> |
                    <a class="btn btn-info" href="~/Dentistas/Details/@item.IdDentista "> Detalles </a> |
                    <a class="btn btn-danger" href="~/Dentistas/Delete/@item.IdDentista"> Eliminar </a>
                    @*<a asp-action="Edit" asp-route-id="@item.IdDentista">Edit</a> |
        <a asp-action="Details" asp-route-id="@item.IdDentista">Details</a> |
        <a asp-action="Delete" asp-route-id="@item.IdDentista">Delete</a>*@
                </td>
            </tr>
        }
    </tbody>
</table>

