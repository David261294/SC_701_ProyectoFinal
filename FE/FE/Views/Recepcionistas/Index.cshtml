@model IEnumerable<FE.Models.Recepcionista>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreRecepcionista)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApellidoRecepcionista)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contraseña)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NombreRecepcionista)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ApellidoRecepcionista)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Contraseña)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.IdRecepcionista">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.IdRecepcionista">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.IdRecepcionista">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
