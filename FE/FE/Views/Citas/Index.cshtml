@model IEnumerable<FE.Models.Citas>

@{
    ViewData["Title"] = "Index";
}

<h1>Lista de Datos</h1>

<p>
    <a class="btn btn-primary" href="~/Citas/Create"> Crear Nuevo </a>
    @*<a asp-action="Create">Crear Nuevo </a>*@
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombrePacienteCita)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApellidoPacienteCita)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DíaCita)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MesCita)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AñoCita)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraCita)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NombrePacienteCita)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ApellidoPacienteCita)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DíaCita)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MesCita)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AñoCita)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HoraCita)
                </td>
                <td>
                    <a class="btn btn-warning" href="~/Citas/Edit/@item.IdCitas "> Editar </a> |
                    <a class="btn btn-info" href="~/Citas/Details/@item.IdCitas "> Detalles </a> |
                    <a class="btn btn-danger" href="~/Citas/Delete/@item.IdCitas"> Eliminar </a>
                    @*<a asp-action="Edit" asp-route-id="@item.IdCitas">Editar</a> |
                    <a asp-action="Details" asp-route-id="@item.IdCitas">Detalles</a> |
                    <a asp-action="Delete" asp-route-id="@item.IdCitas">Eliminar</a>*@
                </td>
            </tr>
        }
    </tbody>
</table>

